                Options Indexes FollowSymLinks Includes ExecCGI
                Order allow,deny
                allow from all
             </Directory>
             LogLevel warn
<%= "        CustomLog #{@params[:CustomLog]}" %>
<%= "        ErrorLog #{@params[:ErrorLog]}" %>
</VirtualHost>
