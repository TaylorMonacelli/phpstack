<%= "<VirtualHost *:#{@params[:Port]}>" %>
<%= "        ServerName #{@params[:ServerName]}" %>
<%= "        ServerAdmin #{@params[:ServerAdmin]}" %>
<%= "        DocumentRoot #{@params[:DocumentRoot]}" %>
<%= "        ServerAlias #{@params[:ServerAlias]}" %>
             ServerSignature On
<%= "        <Directory #{@params[:DocumentRoot]}>" %>
